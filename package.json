{
  "name": "gestion-expedientes-cfe-server",
  "version": "1.0.0",
  "description": "Backend server para gesti√≥n de expedientes CFE con Node.js, Express y TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "npm run dev",
    "migrate": "npm run migrate:up",
    "migrate:up": "npx migrate up",
    "migrate:all": "echo 'a' | npx migrate up",
    "migrate:down": "npx migrate down",
    "migrate:create": "npx migrate create",
    "migrate:list": "npx migrate list",
    "migrate:prune": "npx migrate prune",
    "seed": "npm run seed:user && npm run seed:catalogs",
    "seed:user": "ts-node -r tsconfig-paths/register src/scripts/seed-initial-user.ts",
    "seed:catalogs": "ts-node -r tsconfig-paths/register src/scripts/seed-catalogs.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testMatch='**/__tests__/**/*.e2e.test.ts'"
  },
  "keywords": [
    "express",
    "typescript",
    "nodejs",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "awilix": "^12.0.5",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.2",
    "multer": "^2.0.2",
    "pino": "^10.1.0",
    "pino-loki": "^3.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-migrate-mongoose": "^4.2.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
